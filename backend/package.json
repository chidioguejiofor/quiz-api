{
  "name": "pii-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test:one-unit": "cross-env NODE_ENV=test NODE_PATH=src mocha --timeout 7000 --exit",
    "test:unit": "npm run test:one-unit --  'src/**/*.unit.test.ts'",
    "test:one-integration": "cross-env NODE_ENV=test NODE_PATH=src mocha  -r ts-node/register -r src/__tests__/hooks.ts --timeout 20000 --exit",
    "test:integration": "npm run test:one-integration -- 'src/**/*.integration.test.ts' ",
    "create-domain": "node scripts/create-domain.js ",
    "dev": "cross-env NODE_PATH=src  nodemon --exec ts-node src/index.ts",
    "fix-lint": "eslint . --fix",
    "check-lint": "eslint . ",
    "build": "tsc && find dist -name __tests__ -type d -print0|xargs -0 rm -r --",
    "heroku-postbuild": "npm run build && cross-env NODE_ENV=heroku  node_modules/sequelize-cli/lib/sequelize db:migrate",
    "start": "NODE_PATH=. node index.js || NODE_PATH=dist node ./dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-and-time": "^2.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.1",
    "uuid": "^8.3.2",
    "validatorjs": "^3.22.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.0",
    "@types/chai-subset": "^1.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.0.0",
    "@types/uuid": "^8.3.4",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chai-subset": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.3.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
